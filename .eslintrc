{
    "root": true,
    "plugins": ["security"],
    "extends": ["eslint:recommended", "plugin:security/recommended"],
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "rules": {
        "no-console": [0]
    }
}
